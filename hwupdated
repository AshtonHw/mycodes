package com.perscholas;

public class Homework {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int age = 14;

		if (age < 16)
			System.out.println("cant drive");
		else if (age < 18)
			System.out.println("cant vote");
		else if (age < 25)
			System.out.println("cant rent a car");
		else if (age >= 25)
			System.out.println("do anything legal");

	}
}
